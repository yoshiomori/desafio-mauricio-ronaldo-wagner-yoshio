#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(int argc, char **argv){
  FILE *file = NULL;
  char linha[100];
  if (argc != 2){
    printf ("USO: ondas <nome do arquivo>\n");
    exit(EXIT_FAILURE);
  }
  file = fopen(argv[1], "r");
  if(!file){
    perror("Arquivo de entrada");
    exit(EXIT_FAILURE);
  }
  fscanf(file, "%s", &linha);
  printf("%s\n", linha);
  printf("%s\n", strtok(linha, "(,"));
  return 0;
}
